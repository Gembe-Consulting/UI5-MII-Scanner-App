<mvc:View controllerName="com.mii.scanner.controller.action.FinishOperation" xmlns="sap.m" xmlns:c="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Page title="{i18n>titleFinishOperation}" showNavButton="true" navButtonPress="onNavBack">
		<customHeader>
			<Toolbar id="header">
				<ToolbarSpacer/>
				<c:Icon id="stockTransferPageIcon" src="sap-icon://offsite-work" size="2rem" color="#FFAC00" alt="GI"/>
				<c:Icon id="stockTransferPageIcon2" src="sap-icon://shipping-status" size="1.25rem" color="#FFAC00" class="sapUiSmallMarginEnd"/>
				<Title id="stockTransferPageTitle" text="{i18n>titleStockTransfer}" class="suwStockTransferTitle"/>
				<ToolbarSpacer/>
				<Button id="clearFormButton" icon="sap-icon://redo" press="onClearFormPress"/>
			</Toolbar>
		</customHeader>s
		<content>
			<f:SimpleForm id="dataEntry" title="{i18n>titleFormFinishOperation}" editable="true" layout="ResponsiveGridLayout">
				<f:content>
					<Label text="{i18n>lableTextFormFinishOperationOrderNumber}"/>
					<Input value="{ORDERID}"/>
					<Label text="DateTimePicker"/>
					<DateTimePicker></DateTimePicker>
					<Label text="ComboBox"/>
					<ComboBox showSecondaryValues="{= !${device>/system/phone} }">
						<items>
							<c:ListItem key="1" text="ABCD" additionalText="Das AhBeCe"/>
							<c:ListItem key="2" text="0001" additionalText="null null null eins"/>
							<c:ListItem key="3" text="STPO" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="4" text="AFKO" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="5" text="AUFK" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="6" text="RESB" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="7" text="AFVC" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="8" text="AFVV" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="9" text="AFVU" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="10" text="AFFL" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="11" text="AFFH" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="12" text="AFRU" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="13" text="MSEG" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="14" text="MKPF" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="15" text="CRHD" additionalText="Eine SAP Tabelle"/>
							<c:ListItem key="16" text="CRCA" additionalText="Eine SAP Tabelle"/>
						</items>
					</ComboBox>
				</f:content>
			</f:SimpleForm>
		</content>
		<footer>
			<Toolbar id="footer">
				<ToolbarSpacer/>
				<Button id="saveButton" type="Accept" text="{i18n>textButtonSaveFinishOperation}" press="onSave"/>
				<Button id="cancelButton" type="Reject" text="{i18n>textButtonCancelFinishOperation}" press="onCancelAction"/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>